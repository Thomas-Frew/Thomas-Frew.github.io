---
import BaseLayout from '../layouts/BaseLayout.astro';
const pageTitle = "Projects"

const dateOne = new Date('07/21/2013');
const dateTwo = new Date();

const milliseconds = dateTwo.getTime() - dateOne.getTime(); 
const minutes = Math.ceil(milliseconds/60000);
const hours = Math.ceil(minutes/60);
const days = Math.ceil(hours/24);
const years = Math.ceil(days/365.25);
---

<BaseLayout pageTitle={pageTitle}>
	<p>Proud programmer for {years} years, {days%365} days, {hours%24} hours and {minutes%60} minutes.</p>
</BaseLayout>